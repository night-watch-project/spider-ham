{
  "private": true,
  "name": "sponge",
  "description": "Hassle-free web scraping service.",
  "version": "1.0.0-canary.1",
  "author": "Night Watch Project",
  "license": "MIT",
  "repository": "git@github.com:night-watch-project/sponge.git",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "start": "NODE_ENV=dev nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "NODE_ENV=dev nest start --watch --debug",
    "start:prod": "NODE_ENV=prod node dist/src/main",
    "docker:build:app": "docker build -t nightwatchproject/sponge .",
    "docker:start:prod": "docker run -d --rm --name sponge -p 3000:3000 nightwatchproject/sponge"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.2",
    "@nestjs/platform-fastify": "^7.4.2",
    "@nestjs/swagger": "^4.6.0",
    "@night-watch-project/metascraper-readability": "github:night-watch-project/metascraper-readability#v0.6.1",
    "axios": "^0.20.0-0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "fastify-swagger": "^3.3.0",
    "jsdom": "^16.4.0",
    "metascraper": "^5.14.3",
    "metascraper-audio": "^5.14.3",
    "metascraper-author": "^5.14.3",
    "metascraper-date": "^5.14.3",
    "metascraper-description": "^5.14.3",
    "metascraper-iframe": "^5.14.3",
    "metascraper-image": "^5.14.3",
    "metascraper-lang": "^5.14.3",
    "metascraper-logo": "^5.14.3",
    "metascraper-logo-favicon": "^5.14.3",
    "metascraper-publisher": "^5.14.3",
    "metascraper-title": "^5.14.3",
    "metascraper-url": "^5.14.3",
    "metascraper-video": "^5.14.3",
    "playwright-firefox": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@types/jsdom": "^16.2.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  }
}
